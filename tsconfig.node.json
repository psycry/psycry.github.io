{
  "compilerOptions": {
    "composite": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "types": ["node"],
    "noImplicitAny": false
  },
  "include": ["vite.config.ts"]
}